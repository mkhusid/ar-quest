<html>

<head>
	<meta charset="UTF-8" />
	<script src="https://aframe.io/releases/0.6.0/aframe.min.js"></script>
	<script src="https://cdn.rawgit.com/jeromeetienne/AR.js/1.5.5/aframe/build/aframe-ar.js"></script>
	<script>
		THREEx.ArToolkitContext.baseURL = 'https://rawgit.com/jeromeetienne/ar.js/master/three.js/'
	</script>
</head>

<body style='margin : 0px; overflow: hidden;'>
	<a-scene embedded arjs='sourceType: webcam; detectionMode: mono_and_matrix; matrixCodeType: 3x3;'>
			<a-assets>
			
					<a-asset-item id="tomograf_obj" src="https://raw.githubusercontent.com/mkhusid/ar-quest/master/views/pages/models/tomograf.obj">
					<a-asset-item id="tomograf_mtl" src="https://raw.githubusercontent.com/mkhusid/ar-quest/master/views/pages/models/tomograf.mtl">
		
				</a-assets>
				
		
				<a-marker type="pattern" url="https://raw.githubusercontent.com/mkhusid/ar-quest/master/views/pages/markers/p2.patt">
					<a-obj-model src="#tomograf_obj" mtl="#tomograf_mtl"  scale="1 1 1 "  ></a-obj-model>
				</a-marker>

		<a-entity camera></a-entity>
	</a-scene>
</body>

</html>